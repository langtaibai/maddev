
public class Car {
	 String name;//车名
     double  rent;//租金
    public String getName(){
            return name;
    }
    public void setName(String carName){
            this.name=carName;
    }
    public double getRent(){
            return  rent;
    }
    public void setRent(String carRent){
        this.name=carRent;
    }
}
